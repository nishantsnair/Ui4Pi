<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="ttk.Frame" id="options">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="pack">
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Button" id="update">
        <property name="command" type="command" cbtype="simple">runUpdate</property>
        <property name="compound">top</property>
        <property name="image">update.png</property>
        <property name="text" translatable="yes">Update</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">40</property>
          <property name="pady">40</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="exit">
        <property name="command" type="command" cbtype="simple">closeUI</property>
        <property name="compound">top</property>
        <property name="cursor">arrow</property>
        <property name="image">exit.png</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">Exit</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">40</property>
          <property name="pady">40</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
